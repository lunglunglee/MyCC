            @Html.DisplayNameFor(model => model.貨品名稱)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.存貨)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.貨品名稱)
        </td>
